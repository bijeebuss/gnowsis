{
  "name": "tldr",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "worker": "tsx workflows/worker.ts",
    "email-worker": "tsx workflows/email-worker.ts",
    "combined-worker": "tsx workflows/combined-worker.ts",
    "init-email-schedule": "tsx workflows/init-email-schedule.ts",
    "api": "tsx server/app.ts",
    "temporal": "temporal server start-dev --db-filename .temporal.db",
    "postgres:start": "sudo pg_ctlcluster 15 main start",
    "postgres:stop": "sudo pg_ctlcluster 15 main stop",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:format": "prisma format",
    "prestart": "sudo pg_ctlcluster 15 main start 2>/dev/null || true",
    "start": "concurrently --kill-others --names \"TEMPORAL,VITE,API,WORKER,EMAIL\" --prefix-colors \"blue,cyan,green,yellow,magenta\" \"npm run temporal\" \"npm run dev\" \"npm run api\" \"npm run worker\" \"npm run email-worker\"",
    "stop": "sudo pg_ctlcluster 15 main stop 2>/dev/null || true"
  },
